        "étapes": [
            {
                "numero": 1,
                "titre": "Step 1: \"U-Design\"",
                "solutions": [
                    {
                        "objectif": "",
                        "materiaux_outils": [],
                        "etapes": [
                            {
                                "titre": "",
                                "sous_etapes": [
                                    "Open TinkerCAD software online",
                                    "Go to the text tool and edit to the letters \"F\" and \"U\" in the different scenarios.",
                                    "In the top right-hand corner of TinkerCAD, send to fusion 360.",
                                    "Create a sketch on the top face of the letter \"U\" .",
                                    "Using the overall slot tool, sketch a slot \"12mm\" long and \"2.5mm\" wide.",
                                    "Extrude the sketch of the slot by 5mm in accordance to the thickness of the Crocs measured with the Caliper.",
                                    "Create a sketch on the top face of the extruded slot and offset the slot by \"2mm\".",
                                    "Extrude the offset sketch by \"2mm\".",
                                    "Fillet the top face of the extruded sketch by \"2mm\"."
                                ]
                            }
                        ],
